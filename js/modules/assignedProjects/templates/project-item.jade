#project-list
    table.table
        thead
            tr
                th(style="text-align:center").success
                    |Denumire Proiect
                th(style="text-align:center").success
                    |Descriere Proiect
                th(style="text-align:center").success
                    |Status
                th(style="text-align:center").success
                    |update
        tbody
            tr
                td(style="text-align:center")
                    <%- obj.model.name %>
                td(style="text-align:center")
                    <%- obj.model.description %>
                td(style="text-align:center")
                    <%- obj.model.status %>
                td(style="text-align:center")
                    a(href!='#update-project/<%- model.id %>')
                        img(src="images/update.png" style="width:16px;height:16px")

