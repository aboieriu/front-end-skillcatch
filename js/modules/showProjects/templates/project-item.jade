#project-list
    table.table
        thead
            tr
                th(style="text-align:center")
                    |Denumire Proiect
                th(style="text-align:center")
                    |Descriere Proiect
                th(style="text-align:center")
                    |Status
                th(style="text-align:center")
                    |update
        tbody
            tr
                td(style="text-align:center")
                    a(href!='#show-project/<%- model.id %>')
                        <%- model.name %>
                td(style="text-align:center")
                    <%-model.descriptions %>
                td(style="text-align:center")
                    <%- model.status %>
                td(style="text-align:center")
                    a(href!='#update-project/<%- model.id %>')
                        img(src="images/update.png" style="width:16px;height:16px")