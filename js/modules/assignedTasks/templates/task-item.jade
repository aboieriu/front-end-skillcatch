table.table
    thead
        tr
            th(style="text-align:center").success
                |Denumire Task
            th(style="text-align:center").success
                |Descriere Task
            th(style="text-align:center").success
                |Status
            th(style="text-align:center").success
                |Update
    tbody
        tr
            td(style="text-align:center")
                    <%- obj.model.name %>
            td(style="text-align:center")
                <%- obj.model.description %>
            td(style="text-align:center")
                <%- obj.model.status %>
            td(style="text-align:center")
                a(href!='#userUpdate-task/<%- obj.model.id %>')
                    img(src="images/update.png" style="width:16px;height:16px")